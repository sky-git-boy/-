}
.exam_over {
  width: 90%;
  margin: 40rpx auto;
  border-radius: 20rpx;
  box-shadow: 1px 1px 3px #7c7272,
                1px -1px 3px #dedede,
                -1px 1px 3px #dedede,
                -1px -1px 3px #dedede;
  -moz-box-shadow: 1px 1px 3px #7c7272,
                     1px -1px 3px #dedede,
                     -1px 1px 3px #dedede,
                     -1px -1px 3px #dedede;
