  -webkit-flex-direction: column;
          flex-direction: column;
}
.exam_item {
  border: 1rpx solid #dedede;
  width: 300rpx;
  padding: 20rpx;
  border-radius: 50rpx;
  margin: 20rpx auto;
  background-color: #fff;
  font-size: 30rpx;
  line-height: 1 ;
  box-sizing:none;
}
.exam_xuan button::after{
  width: 0 !important;
  height: 0 !important;
}
